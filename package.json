{
  "name": "ant-game-tests",
  "version": "1.0.0",
  "description": "Test suite for the Ant Game project",
  "main": "sketch.js",
  "scripts": {
    "test": "node tests/runners/runAllTests.js",
    "test:all": "node tests/runners/runAllTests.js",
    "test:browser": "node tests/runners/runBrowserTests.js",
    "test:jobs": "node tests/runners/runAllJobTests.js",
    "test:unit": "node tests/runners/runAllUnitTests.js",
    "test:master": "node tests/runners/runMasterTestSuite.js",
    "test:fix-paths": "node tests/runners/fixTestPaths.js",
    "test:integration": "node tests/integration/jobSystemIntegration.test.js && node tests/integration/gameplayIntegration.test.js",
    "test:integration:jobs": "node tests/integration/jobSystemIntegration.test.js",
    "test:integration:gameplay": "node tests/integration/gameplayIntegration.test.js",
    "test:quick": "npm run test:jobs && npm run test:integration:jobs",
    "test:setup": "cd tests/e2e && pip install -r requirements.txt",
    "test:python": "python tests/e2e/steps/run_python_tests.py",
    "test:python:core": "behave --steps-dir tests/e2e/steps tests/e2e/features --tags @core",
    "test:python:ui": "behave --steps-dir tests/e2e/steps tests/e2e/features --tags @ui",
    "test:python:browser": "behave --steps-dir tests/e2e/steps tests/e2e/features --tags @browser",
    "test:python:legacy": "python tests/e2e/steps/legacy_system_steps.py",
    "test:python:buttons": "behave --steps-dir tests/e2e/steps tests/e2e/features --tags @buttons",
    "test:python:integration": "behave --steps-dir tests/e2e/steps tests/e2e/features --tags @integration",
    "test:python:red-flags": "python tests/e2e/steps/detect_red_flags.py",
    "test:python:all": "npm run test:python:setup && npm run test:python:red-flags && npm run test:python",
    "test:bdd": "python tests/e2e/run_bdd_tests.py",
    "test:bdd:ants": "python tests/e2e/run_bdd_tests.py --tags @ants",
    "test:bdd:buttons": "python tests/e2e/run_bdd_tests.py --tags @buttons",
    "test:bdd:ui": "python tests/e2e/run_bdd_tests.py --tags @ui",
    "test:bdd:integration": "python tests/e2e/run_bdd_tests.py --tags @integration",
    "test:bdd:core": "python tests/e2e/run_bdd_tests.py --tags @core",
    "dev": "python -m http.server 8000",
    "start": "python -m http.server 8000"
  },
  "keywords": [
    "game",
    "ants",
    "simulation",
    "p5js",
    "testing"
  ],
